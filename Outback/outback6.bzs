SetScrap,1,40
SetScrap,5,40
CHECK:
IsAround,Recycler
IfEQ,true,CHECK
Clear
Display,lose_recy,red
Fail,10,"outback6lr.txt"

[routine,Defend_Enemy_Recy,1,true]
GetByLabel,Enemy_Recycler,"enemy_recy"
Createp,Scavenger,"fbscup",5,"scav1"
Createp,Scavenger,"fbscup",5,"scav2"
Createp,Scavenger,"fbscup",5,"scav3"
CHECK:
GetCurHealth,Enemy_Recycler
IfGT,5500,CHECK
Createp,Titan1,"fvatank",5,"titan1"
Createp,Titan2,"fvatank",5,"titan2"
Createp,Titan3,"fvatank",5,"titan3"
Createp,Titan4,"fvatank",5,"titan4"
Stop,Titan1,1
Stop,Titan2,1
Stop,Titan3,1
Stop,Titan4,1
Createp,Warrior1,"fvtankout2",5,"tanks"
Createp,Warrior2,"fvtankout2",5,"tanks"
Createp,Warrior3,"fvtankout2",5,"tanks"
Createp,Warrior4,"fvtankout2",5,"tanks"
Patrol,Warrior1,"patrol",1
Patrol,Warrior2,"patrol",1
Patrol,Warrior3,"patrol",1
Patrol,Warrior4,"patrol",1

[routine,NewObjectRoutine,1,true]
OnNewObject,100,NewObjectRoutine,NewUnit
GetCompForce,TheCompForce
Wait,600
START:
RunSpeed,NewObjectRoutine,0,true
GetTeam,NewUnit
IfNE,5,START
Set,TheCompForce,3
Add,TheCompForce,0
IfEQ,1,SMALL_FORCE
IfEQ,2,MED_FORCE
IfEQ,3,LARGE_FORCE
SMALL_FORCE:
SetSkill,NewUnit,2
JumpTo,BEGIN
MED_FORCE:
SetSkill,NewUnit,3
JumpTo,BEGIN
LARGE_FORCE:
SetSkill,NewUnit,4
JumpTo,BEGIN
BEGIN:
IsODF,NewUnit,"fvscout"
IfEQ,false,NOT_SCOUT
GiveWeapon,NewUnit,"gquill_c"
GiveWeapon,NewUnit,"gshield"
JumpTo,START
NOT_SCOUT:
IsODF,NewUnit,"fvtank"
IfEQ,false,NOT_TANK
GiveWeapon,NewUnit,"garc_c"
GiveWeapon,NewUnit,"gshield"
JumpTo,START
NOT_TANK:
IsODF,NewUnit,"fvsent"
IfEQ,false,NOT_SENTRY
GiveWeapon,NewUnit,"ggaus"
GiveWeapon,NewUnit,"gshield"
NOT_SENTRY:
IsODF,NewUnit,"fvarch"
IfEQ,false,START
GiveWeapon,NewUnit,"gabsorb"
JumpTo,START

