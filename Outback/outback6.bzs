GetByLabel,marker3,"marker3"
GetByLabel,marker4,"marker4"
GetPos,marker1,pilot1
GetPos,marker2,pilot2
GetPos,marker3,pilot3
GetPos,marker4,pilot4
Remove,marker1
Remove,marker2
Remove,marker3
Remove,marker4

[routine,BombPlayer,1,true]
GetByLabel,Stronghold,"strong"
GetByLabel,BombMarker1,"bomb1"
GetByLabel,BombMarker2,"bomb2"
GetByLabel,BombMarker3,"bomb3"
GetByLabel,BombMarker4,"bomb4"
GetPos,BombMarker1,Bomb1
GetPos,BombMarker2,Bomb2
GetPos,BombMarker3,Bomb3
GetPos,BombMarker4,Bomb4
AddToPos,0,75,0,Bomb1
AddToPos,0,75,0,Bomb2
AddToPos,0,75,0,Bomb3
AddToPos,0,75,0,Bomb4
Remove,BombMarker1
Remove,BombMarker2
Remove,BombMarker3
Remove,BombMarker4
Wait,400
START:
RandJump,BOMBER1,BOMBER2,BOMBER3,BOMBER4
JumpTo,DELAY
BOMBER1:
Create,DayWrecker,"apwrckout1",5,Bomb1
JumpTo,DELAY
BOMBER2:
Create,DayWrecker,"apwrckout1",5,Bomb2
JumpTo,DELAY
BOMBER3:
Create,DayWrecker,"apwrckout1",5,Bomb3
JumpTo,DELAY
BOMBER4:
Create,DayWrecker,"apwrckout1",5,Bomb4
DELAY:
Wait,80
IsAround,Stronghold
IfEQ,true,START

[routine,Furies,1,true]
Wait,4
Ally,5,4
GetPos,Enemy_Recycler,Fury_Tanks
WAITING:
IsAround,Rocks
IfEQ,true,WAITING
SetPlan,"outback6c.aip",4
Wait,120
Audio,"outback6sav.wav"
START:
Createp,Fury,"fvsav",4,"sav1"
Createp,Fury,"fvsav",4,"sav1"
Createp,Fury,"fvsav",4,"sav2"
Createp,Fury,"fvsav",4,"sav2"
Createp,Fury,"fvsav",4,"sav3"
Createp,Fury,"fvsav",4,"sav3"
//Createp,Fury,"fvsav",4,"sav4"
//Createp,Fury,"fvsav",4,"sav4"
