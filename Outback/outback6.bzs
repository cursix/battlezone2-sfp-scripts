[routine,Main,1,true]
Wait,1
GetByLabel,Rocks,"rocks"
Display,Start,white
Audio,"outback6a.wav"
RECYCLER:
IsDeployed,Recycler
IfEQ,false,RECYCLER
Wait,10
Quake,500
Display,Earth_Quake,white
Audio,"outback6b.wav"
Wait,5
SetAnimation,Rocks,"landslide",1
StartAnimation,Rocks
CamPath,"cam1",5000,1500,Rocks
Clear
Display,Objective1,white
Audio,"outback6c.wav"
StopQuake
SetPlan,"outback6a.aip",5
Wait,600
Clear
Display,Objective2,white
Audio,"outback6d.wav"
Ally,1,2
Createp,Demolisher,">ivconsout1",2,"cons"
BeaconOn,Demolisher
START:
DistObject,Player,Demolisher
IfGT,81,START
Clear
Display,Objective2,green
Wait,1
Display,Objective3,white
Audio,"outback6e.wav"
Gotoo,Demolisher,Rocks,1
ATTACK_CONST:
DistPath,Demolisher,"raid"
IfGT,100,ATTACK_CONST
Create,Scion_Pilot,">fssoldout1",5,pilot1
Attack,Scion_Pilot,Demolisher,1
Create,Scion_Pilot,"fssoldout1",5,pilot1
Attack,Scion_Pilot,Demolisher,1
Create,Scion_Pilot,"fssoldout1",5,pilot2
Attack,Scion_Pilot,Demolisher,1
Create,Scion_Pilot,"fssoldout1",5,pilot2
Attack,Scion_Pilot,Demolisher,1
Create,Scion_Pilot,"fssoldout1",5,pilot3
Attack,Scion_Pilot,Demolisher,1
Create,Scion_Pilot,"fssoldout1",5,pilot3
Attack,Scion_Pilot,Demolisher,1
Create,Scion_Pilot,"fssoldout1",5,pilot4
Attack,Scion_Pilot,Demolisher,1
Create,Scion_Pilot,"fssoldout1",5,pilot4
Attack,Scion_Pilot,Demolisher,1
CHECK_CONST:
IsAround,Demolisher
IfEQ,false,DEAD
DistObject,Demolisher,Rocks
IfGT,100,CHECK_CONST
Stop,Demolisher,1
Wait,1
GetPos,Rocks,rocks
Remove,Rocks
Create,Effect1,"demolish",0,rocks
JumpTo,DONE
DEAD:
Clear
Display,Objective3,red
Fail,10,"outback6ld.txt"
JumpTo,END
DONE:
SetPlan,"outback6b.aip",5
Clear
Display,Objective3,green
Wait,1
Display,Objective4,white
audio,"outback6f.wav"
Wait,2
Remove,Demolisher
CHECK_ENEMY:
IsAround,Enemy_Recycler
IfEQ,true,CHECK_ENEMY
GetByTS,Enemy_Factory,5,2
CHECK_ENEMYFACT:
IsAround,Enemy_Factory
IfEQ,true,CHECK_ENEMYFACT
Clear
Display,Objective4,green
Audio,"outback6g.wav"
Succeed,10,"outback6w.txt"
END:

